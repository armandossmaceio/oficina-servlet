<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Principal</servlet-name>
  	<display-name>Principal</display-name>
  	<description>Trata as requisições da aplicação</description>
  	<servlet-class>servicos.Principal</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>servicos.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Principal</servlet-name>
  	<url-pattern>/principal</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
</web-app>
